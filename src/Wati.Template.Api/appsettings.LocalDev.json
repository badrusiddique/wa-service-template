{
  "Serilog": {
    "Properties": {
      "Environment": "LocalDev"
    }
  },
  "Authentication": {
    "IsEnabled": false,
    "SkipCertificateCheck": true
  },
  "ConnectionStrings": {
    "autoMigrationEnabled": true
  },
  "ExternalSource": {
    "Redis": {
      "Ssl": false,
      "Hosts": [
        {
          "Host": "localhost",
          "Port": "6379"
        }
      ]
    }
  }
}